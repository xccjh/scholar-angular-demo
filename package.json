{
  "name": "ky-scholar-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8991 --host 0.0.0.0",
    "start:test": "cross-env NODE_ENV=test npm run start",
    "start:production": "cross-env NODE_ENV=production npm run start",
    "start:intranet": "cross-env NODE_ENV=intranet npm run start",
    "build": "ng build --prod --aot --configuration=development",
    "build:development": "cross-env NODE_ENV=development npm run build",
    "build:test": "cross-env NODE_ENV=test npm run build -- --configuration=test",
    "build:intranet": "cross-env NODE_ENV=intranet npm run build -- --configuration=intranet",
    "build:production": " npm run build -- --build--optimizer --configuration=production",
    "server": "node server.js",
    "server:https": "node server.js https=true",
    "file": "node fileServer.js",
    "oss:testExtend": "node checkCommit.js && npm run build:test && npm run uploadOssTestExtend && npm run uploadOssTestExtendBak",
    "oss:prodExtend": "node checkCommit.js && npm run build:production && npm run uploadOssProdExtend && npm run uploadOssProdExtendBak",
    "oss:test": "npm run build:test && npm run uploadOssTest && npm run uploadOssTestBak",
    "oss:prod": "npm run build:production && npm run uploadOssProd && npm run uploadOssProdBak",
    "uploadOssTestExtend": "node upload-oss-test.js ky-web-test1 scholar dist/ky-scholar-web upload",
    "uploadOssTestExtendBak": "node upload-oss-test.js ky-file-test html/scholar dist/ky-scholar-web upload",
    "uploadOssProdExtend": "node upload-oss-prod.js qkc-web scholar dist/ky-scholar-web upload",
    "uploadOssProdExtendBak": "node upload-oss-prod.js qkc-file html/scholar dist/ky-scholar-web upload",
    "uploadOssTest": "node uploadOssTest.js bucket=ky-web-test1 ossDir=scholar buildDir=./dist/ky-scholar-web",
    "uploadOssTestBak": "node uploadOssTest.js bucket=ky-file-test ossDir=html/scholar buildDir=./dist/ky-scholar-web",
    "uploadOssProd": "node uploadOssProd.js bucket=qkc-web ossDir=scholar buildDir=./dist/ky-scholar-web",
    "uploadOssProdBak": "node uploadOssProd.js bucket=qkc-file ossDir=html/scholar buildDir=./dist/ky-scholar-web",
    "fixedPointPublish": "node fixedPointPublish.js 2021-03-24 01:00:00",
    "durationPointPublish": "node durationPointPublish.js Day=1 Hour=1 Min=59 Sec=59",
    "fixedPointPublishTest": "npm run fixedPointPublish && npm run oss:test",
    "durationPointPublishTest": "npm run durationPointPublish && npm run oss:test",
    "fixedPointPublishTestExtend": "npm run fixedPointPublish && npm run oss:testExtend",
    "durationPointPublishTestExtend": "npm run durationPointPublish && npm run oss:testExtend",
    "fixedPointPublishProd": "npm run fixedPointPublish && npm run oss:prod",
    "durationPointPublishProd": "npm run durationPointPublish && npm run oss:prod",
    "fixedPointPublishProdExtend": "npm run fixedPointPublish && npm run oss:prodExtend",
    "durationPointPublishProdExtend": "npm run durationPointPublish && npm run oss:prodExtend",
    "bundle-report": "ng build --prod --aot --stats-json",
    "analyze-report": "webpack-bundle-analyzer dist/ky-scholar-web/stats-es2015.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@ckeditor/ckeditor5-angular": "^1.2.3",
    "@ngrx/component-store": "^10.0.0",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.0",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/router": "^4.2.0",
    "@xccjh/xccjh-ckeditor5-video-file-upload": "^4.0.0",
    "angular2-qrcode": "^2.0.3",
    "connect-history-api-fallback": "^1.6.0",
    "crypto-js": "^3.1.2",
    "echarts": "^4.9.0",
    "js-base64": "^3.5.2",
    "ng-zorro-antd": "^9.3.0",
    "ngx-clipboard": "^14.0.1",
    "ngx-echarts": "^5.1.2",
    "ngx-videogular": "^9.0.1",
    "node-schedule": "^1.3.3",
    "rxjs": "^6.5.5",
    "screenfull": "^5.0.0",
    "tslib": "^1.14.0",
    "videogular2": "^7.0.2",
    "viewerjs": "^1.7.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.2.0",
    "@angular-devkit/build-angular": "^0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@types/core-js": "^2.5.4",
    "@types/echarts": "^4.8.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.66",
    "@xccjh/check-commit": "^1.0.0",
    "@xccjh/git-pull": "^1.0.1",
    "@xccjh/timer-publish": "^1.0.4",
    "ali-oss": "^6.10.0",
    "codelyzer": "^5.1.2",
    "compression-webpack-plugin": "^6.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv-webpack": "^5.1.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.5",
    "https": "^1.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "multer": "^1.4.2",
    "prettier": "^2.1.2",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-merge": "^4.2.2"
  }
}
